[
  {
    "name": "少数派",
    "pattern": "https://sspai.com/post/*",
    "pick": ".article-body > .articleWidth-content > .content",
    "hide": [
      ".bgGrey",
      ".article-side"
    ],
    "chAttr": [
      {
        "type": "assign.from.self-attr",
        "pick": ".content img",
        "attr": "src",
        "tAttr": "data-original"
      }
    ],
    "tags": [
      "news"
    ],
    "contributors": [
      "Mika"
    ]
  }
]

