[
  {
    "name": "凤凰网/资讯",
    "pattern": "https://news.ifeng.com/c/*",
    "version": 20220503,
    "pick": [
      "[class|=main_content]",
      "[class|=artical]",
      "[class|=article]"
    ],
    "hide": [
      "[class|=rightContent]",
      "[class|=infoboxLink] [class|=face]",
      "[class|=infoboxLink] [class|=follow]",
