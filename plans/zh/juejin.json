[
  {
    "name": "掘金-文章/资讯",
    "pattern": [
      "https://juejin.cn/post/*"
    ],
    "version": 20240502,
    "contributors": [
      "CodingOX"
    ],
    "actions": [
      {
        "hideSibling": [
          ".article-content pre > code"
        ]
      },
      {
        "pick": ".article-content"
      }
    ],
    "tags": [
      "IT",
      "community",
      "frontend"
    ]
  },
  {
    "name": "掘金-资讯",
    "pattern": [
      "https://juejin.cn/news/*"
    ],
    "version": 20240502,
    "contributors": [
      "Mika"
    ],
    "actions": [
      {
        "hide": [
          ".article #commentListBox",
          ".article .jh-news-detail-action-area > .category",
          ".article .jh-news-detail-action-area > .viewcount"
        ]
      },
      {
        "pick": ".article"
      }
    ],
    "tags": [
      "IT",
      "community",
      "frontend"
    ]
  },
  {
    "name": "掘金-小册",
    "pattern": "https://juejin.cn/book/*/section/*",
    "version": 20240502,
    "contributors": [
      "Sularxx",
      "Mika"
    ],
    "actions": [
      {
        "form": {
          "title": ".article-content > .markdown-body > h1"
        }
      },
      {
        "pick": ".article-content > .markdown-body"
      }
    ],
    "tags": [
      "IT",
      "book"
    ]
  }
]