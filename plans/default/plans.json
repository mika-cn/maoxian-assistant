{
  "name": "default",
  "version": 20211006,
  "description": "default plans",
  "plans": [
    {
      "name": "wikipedia.org",
      "pattern": "https://*.wikipedia.org/wiki/*",
      "pick": "div.mw-parser-output",
      "hide": [
        "div#toc",
        "table.infobox",
        "span.mw-editsection",
        "table.navbox",
        "div.thumb",
        "div.dablink",
        "table.metadata"
      ],
      "tags": ["knowledge", "wiki"]
    },
    {
      "name": "Wikipedia Mobile View",
      "pattern": "https://*.m.wikipedia.org/*/",
      "contributors": ["Altair Wei (author)"],
      "pick": "#content",
      "hide": [
        ".page-actions-menu",
        "#toc",
        ".mw-editsection",
        "h2 > div.mw-ui-icon",
        "table.box-Unreferenced",
        "table.box-More_citations_needed"
      ],
      "chAttr": [
        {
          "type": "split2list.add",
          "pick": "h2.section-heading",
          "attr": "style",
          "value": "width: 100%",
          "sep": ";"
        }
      ]
    }
  ]
}
